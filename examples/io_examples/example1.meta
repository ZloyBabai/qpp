metaparam_array{
  ordered = yes;

  metaparam_select(required=yes)
  {
     metaparameter intHren(type=int, required=yes);
     metaparameter doubleHren(type=double, required=yes);
  }

  metaparameter dft(type=string,required=yes)
  {
    value(lda);
    value(b3lyp)
    {
      metaparameter XHF(type=double, required=yes, default=0.2);
      metaparameter GGA_corr(type=double, 
                             required=yes, defaul=0.51);
    }
    value(pbe)
    {
      metaparameter gridsize(type=double,required=yes); 
    }
  }

}
