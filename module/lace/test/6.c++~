#include <iostream>
#include <lace/element.hpp>

using namespace _lace_storage;

int main()
{
  lace::dcomplex z(2,2), I(0,1), x, y;

  x = _complex_reference<lace::dcomplex>(z,true);
  std::cout << x << "\n";

  _complex_reference<lace::dcomplex>(z,true) = I;

  std::cout << "z = " << z << "\n";
}
